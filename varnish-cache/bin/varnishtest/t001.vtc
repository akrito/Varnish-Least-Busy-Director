# Test that we get anything through at all
#
# $Id$

varnish v1 -arg -launch

varnish v1 -vcl {
	backend s1 {
		.host = "localhost";
		.port = "9080";
	}
	vcl_recv {
		pipe;
	}
}

server s1 -repeat 1 {
	rxreq 
	txresp \
		-hdr "Connection: close" \
		-hdr "Test1: foobar" \
		-hdr "Test2: foobar" \
		-body "012345\n"
}

server s1 -start 

client c1 {
	txreq -url "/"
	rxresp
	expect resp.status == 200
}

client c1 -run

server s1 -wait

varnish v1 -stop
