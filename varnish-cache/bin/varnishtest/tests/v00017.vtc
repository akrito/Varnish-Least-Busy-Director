test "VCL compiler coverage test: vcc_acl.c"

varnish v1 -badvcl {
	backend b { .host = "127.0.0.1"; }
	acl a { "10.1.2.3"/33; }
	sub vcl_recv { if (client.ip ~ a) { pass; } }
}

